{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@types/node", "@cloudflare/workers-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"exclude": ["node_modules", "dist", "tests"],
	"include": ["src", "scripts", "index.d.ts"]
}
